<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- statement -->
<mapper namespace="org.itheima.mapper.UserMapper">
    <!--  这里resulttype=User是因为在mybatis-config中设置了typeAliase，从而简化书写  -->
    <select id="selectAll" resultType="org.itheima.pojo.User">
        select * from tb_user;

    </select>
    <select id="selectById" resultType="org.itheima.pojo.User"></select>
</mapper>